# Google Places API (New) - get your key from https://console.cloud.google.com/
GOOGLE_PLACES_API_KEY=your_google_api_key_here

# PostgreSQL
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/places_db
DATABASE_URL_SYNC=postgresql://postgres:postgres@localhost:5432/places_db

# Rate Limiting
MAX_REQUESTS_PER_SECOND=5
PAGINATION_DELAY_SECONDS=2.0

# Enrichment
ENRICHMENT_TIMEOUT=10
ENRICHMENT_MAX_RETRIES=3
RESPECT_ROBOTS_TXT=true

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=false
LOG_LEVEL=INFO

# Redis (optional, for caching)
REDIS_URL=redis://localhost:6379/0
